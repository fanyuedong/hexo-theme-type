head
    meta(http-equiv='content-type', content='text/html; charset=utf-8')
    meta(http-equiv="X-UA-Compatible", content='IE=edge')
    meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0', name='viewport')
    meta(name='description', content=config.description)
    block title
    link(rel='stylesheet', type='text/css', href=url_for(theme.css) + '/app.css' + '?v=' + theme.version)
    link(rel='stylesheet', type='text/css', href='//lib.baomitu.com/normalize/8.0.1/normalize.min.css')

    script(type='text/javascript', src='//lib.baomitu.com/jquery/3.4.0/jquery.min.js')
    script(type='text/javascript', src='https://unpkg.com/better-scroll/dist/bscroll.min.js')
    script(type='text/javascript', src=url_for(theme.js)+'/evil-icons.min.js' + '?v=' + theme.version)
    script(type='text/javascript', src=url_for(theme.js)+'/jquery.fitvids.js' + '?v=' + theme.version)
    script(type='text/javascript', src=url_for(theme.js)+'/app.js' + '?v=' + theme.version)
    link(rel='icon', mask='',sizes='any', href=url_for('favicon.ico'))
    link(rel='Shortcut Icon', type='image/x-icon', href=url_for('favicon.ico'))
    link(rel='apple-touch-icon', href=url_for('apple-touch-icon.png'))


